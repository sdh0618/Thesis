language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:57:52GMT},
date-modified = {2020-07-28T04:34:14GMT},
abstract = {Data from the Fermi-LAT reveal two large gamma-ray bubbles, extending 50{\textdegree} above and below the Galactic center (GC), with a width of about 40{\textdegree} in longitude. The gamma-ray emission associated with these bubbles has a significantly harder spectrum (dN/dE {\textasciitilde} E -2) than the inverse Compton emission from electrons in the Galactic disk, or the gamma rays produced by the decay of pions from proton-interstellar medium collisions. There is no significant spatial variation in the spectrum or gamma-ray intensity within the bubbles, or between the north and south bubbles. The bubbles are spatially correlated with the hard-spectrum microwave excess known as the WMAP haze; the edges of the bubbles also line up with features in the ROSAT X-ray maps at 1.5-2 keV. We argue that these Galactic gamma-ray bubbles were most likely created by some large episode of energy injection in the GC, such as past accretion events onto the central massive black hole, or a nuclear starburst in the last {\textasciitilde}10 Myr. Dark matter annihilation/decay seems unlikely to generate all the features of the bubbles and the associated signals in WMAP and ROSAT; the bubbles must be understood in order to use measurements of the diffuse gamma-ray emission in the inner Galaxy as a probe of dark matter physics. Study of the origin and evolution of the bubbles also has the potential to improve our understanding of recent energetic events in the inner Galaxy and the high-latitude cosmic ray population.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010ApJ...724.1044S&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2010/Su/The%20Astrophysical%20Journal%202010%20Su.pdf},
file = {{The Astrophysical Journal 2010 Su.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2010/Su/The Astrophysical Journal 2010 Su.pdf:application/pdf;The Astrophysical Journal 2010 Su.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2010/Su/The Astrophysical Journal 2010 Su.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/724/2/1044}}
}

