@article{Siegert2018_V5668Sgr,
author = {Siegert, Thomas and Coc, Alain and Delgado, Laura and Diehl, Roland and Greiner, Jochen and Hernanz, Margarita and Jean, Pierre and Jos{\'e}, Jordi and Molaro, Paolo and Pleintinger, Moritz M M and Savchenko, Volodymyr and Starrfield, Sumner and Tatischeff, Vincent and Weinberger, Christoph},
title = {{Gamma-ray observations of Nova Sgr 2015 No. 2 with INTEGRAL}},
