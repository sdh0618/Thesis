title = {{Millisecond pulsar velocities}},
journal = {Monthly Notices},
year = {1999},
volume = {307},
number = {4},
pages = {925--933},
month = aug,
affiliation = {Physics Department, University of Melbourne, Parkville, Victoria 3052, Australia},
doi = {10.1046/j.1365-8711.1999.02685.x},
language = {English},
read = {Yes},
