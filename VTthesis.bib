journal = {The Astrophysical Journal},
year = {2013},
volume = {779},
number = {1},
pages = {57},
month = dec,
publisher = {IOP Publishing},
affiliation = {Research Institute for Science and Engineering, Waseda University, 3-4-1 Okubo, Shinjuku, Tokyo 169-8555, Japan kataoka.jun@waseda.jp},
