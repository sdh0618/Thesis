file = {{The Astrophysical Journal 2013 Kataoka.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Kataoka/The Astrophysical Journal 2013 Kataoka.pdf:application/pdf;The Astrophysical Journal 2013 Kataoka.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Kataoka/The Astrophysical Journal 2013 Kataoka.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/779/1/57}}
}

