@article{Crocker2010_GalCen_50muGauss,
author = {Crocker, Roland M and Jones, David I and Melia, Fulvio and Ott, J{\"u}rgen and Protheroe, Raymond J},
title = {{A lower limit of 50 microgauss for the magnetic field near the Galactic Centre}},
journal = {Nature},
year = {2010},
volume = {463},
number = {7},
pages = {65--67},
month = jan,
publisher = {Nature Publishing Group},
affiliation = {School of Physics, Monash University, Victoria 3110, Australia},
doi = {10.1038/nature08635},
language = {English},
read = {Yes},
rating = {0},
date-added = {2021-08-17T15:52:20GMT},
date-modified = {2021-09-08T12:48:33GMT},
abstract = {The amplitude of the magnetic field near the Galactic Centre has been uncertain by two orders of magnitude for several decades. On a scale of {\textasciitilde}100 parsecs (pc), fields of {\textasciitilde}1,000 microgauss ($\mu$G refs 1-3) have been reported, implying a magnetic energy density more than 10,000 times stronger than typical for the Galaxy. Alternatively, the assumption of pressure equilibrium between the various phases of the Galactic Centre interstellar medium (including turbulent molecular gas, the contested {\textquoteleft}very hot{\textquoteright} plasma, and the magnetic field) suggests fields of {\textasciitilde}100 $\mu$G over {\textasciitilde}400 pc size scales. Finally, assuming equipartition, fields of only {\textasciitilde}6 $\mu$G have been inferred from radio observations for 400 pc scales. Here we report a compilation of previous data that reveals a downward break in the region's non-thermal radio spectrum (attributable to a transition from bremsstrahlung to synchrotron cooling of the in situ cosmic-ray electron population). We show that the spectral break requires that the Galactic Centre field be at least {\textasciitilde}50 $\mu$G on 400 pc scales, lest the synchrotron-emitting electrons produce too much $\gamma$-ray emission, given other existing constraints. Other considerations support a field of 100 $\mu$G, implying that over 10% of the Galaxy's magnetic energy is contained in only <{\textasciitilde}0.05% of its volume.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010Natur.463...65C&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1038/nature08635}}
}

