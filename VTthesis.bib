@article{Gordon2013_GCE,
author = {Gordon, Chris and Macias, Oscar},
title = {{Dark matter and pulsar model constraints from Galactic Center Fermi-LAT gamma-ray observations}},
journal = {Physical Review D},
year = {2013},
volume = {88},
number = {8},
pages = {083521},
month = oct,
publisher = {American Physical Society},
affiliation = {Department of Physics and Astronomy, Rutherford Building, University of Canterbury, Private Bag 4800, Christchurch 8140, New Zealand},
doi = {10.1103/PhysRevD.88.083521},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:30:44GMT},
date-modified = {2020-07-24T04:49:22GMT},
abstract = {Employing Fermi-LAT gamma-ray observations, several independent groups have found excess extended gamma-ray emission at the Galactic Center (GC). Both annihilating dark matter (DM) or a population of ∼103 unresolved millisecond pulsars (MSPs) are regarded as well-motivated possible explanations. However, there are significant uncertainties in the diffuse galactic background at the GC. We have performed a revaluation of these two models for the extended gamma-ray source at the GC by accounting for the systematic uncertainties of the Galactic diffuse emission model. We also marginalize over point-source and diffuse background parameters in the region of interest. We show that the excess emission is significantly more extended than a point source. We find that the DM (or pulsar-population) signal is larger than the systematic errors and therefore proceed to determine the sectors of parameter space that provide an acceptable fit to the data. We find that a population of 1000-2000 MSPs with parameters consistent with the average spectral shape of Fermi-LAT measured MSPs is able to fit the GC excess emission. For DM, we find that a pure $\tau$+$\tau$- annihilation channel is not a good fit to the data. But a mixture of $\tau$+$\tau$- and bb{\textasciimacron} with a {\textlangle}$\sigma$v{\textrangle} of order the thermal relic value and a DM mass of around 20 to 60 GeV provides an adequate fit.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013PhRvD..88h3521G&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Gordon/Physical%20Review%20D%202013%20Gordon.pdf},
