number = {2},
pages = {931--938},
month = feb,
publisher = {IOP Publishing},
affiliation = {Department of Astronomy, University of Wisconsin at Madison, 475 North Charter Street, Madison, WI 53706; Department of Astronomy, University of Virginia, P.O. Box 3818, Charlottesville, VA 22903-0818.},
doi = {10.1086/426679},
language = {English},
read = {Yes},
