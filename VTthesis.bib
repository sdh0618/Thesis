@article{Slatyer2016_CMB_DM,
author = {Slatyer, Tracy R},
title = {{Indirect dark matter signatures in the cosmic dark ages. I. Generalizing the bound on s -wave dark matter annihilation from Planck results}},
journal = {Physical Review D},
year = {2016},
volume = {93},
number = {2},
pages = {023527},
month = jan,
