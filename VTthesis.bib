date-added = {2021-08-26T14:22:22GMT},
date-modified = {2021-09-08T12:48:32GMT},
abstract = {Recent measurements of the cosmic microwave background (CMB) anisotropies by Planck provide a sensitive probe of dark matter annihilation during the cosmic dark ages, and specifically constrain the annihilation parameter feff{\textlangle}$\sigma$ v {\textrangle}/m$\chi$. Using new results (paper II) for the ionization produced by particles injected at arbitrary energies, we calculate and provide feff values for photons and e+e- pairs injected at keV-TeV energies; the feff value for any dark matter model can be obtained straightforwardly by weighting these results by the spectrum of annihilation products. This result allows the sensitive and robust constraints on dark matter annihilation presented by the Planck collaboration to be applied to arbitrary dark matter models with s -wave annihilation. We demonstrate the validity of this approach using principal component analysis. As an example, we integrate over the spectrum of annihilation products for a range of Standard Model final states to determine the CMB bounds on these models as a function of dark matter mass, and demonstrate that the new limits generically exclude models proposed to explain the observed high-energy rise in the cosmic ray positron fraction. We make our results publicly available at http://nebel.rc.fas.harvard.edu/epsilon.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2016PhRvD..93b3527S&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2016/Slatyer/Physical%20Review%20D%202016%20Slatyer.pdf},
file = {{Physical Review D 2016 Slatyer.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2016/Slatyer/Physical Review D 2016 Slatyer.pdf:application/pdf;Physical Review D 2016 Slatyer.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2016/Slatyer/Physical Review D 2016 Slatyer.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevD.93.023527}}
}

@article{Hachisu2006_novae_declinelaw,
