author = {Moskalenko, Igor V and Strong, Andrew W and Mashnik, Stepan G},
title = {{Propagation of Cosmic Rays: Nuclear Physics in Cosmic-Ray Studies}},
booktitle = {INTERNATIONAL CONFERENCE ON NUCLEAR DATA FOR SCIENCE AND TECHNOLOGY. AIP Conference Proceedings},
year = {2005},
pages = {1612--1617},
organization = {NASA/Goddard Space Flight Center, Code 661, Greenbelt, MD 20771; Joint Center for Astrophysics/University of Maryland, Baltimore County, Baltimore, MD 21250},
