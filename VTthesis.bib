@article{Livio1994_nova_abundances,
author = {Livio, Mario and Truran, James W},
title = {{On the interpretation and implications of nova abundances: an abundance of riches or an overabundance of enrichments}},
journal = {Astrophysical Journal},
year = {1994},
volume = {425},
pages = {797--801},
month = apr,
affiliation = {Space Telescope Science Inst., Baltimore, MD, US},
doi = {10.1086/174024},
language = {English},
read = {Yes},
