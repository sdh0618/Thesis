@article{Frey2013_511DMdeex,
author = {Frey, Andrew R and Reid, Nicholas B},
title = {{Cosmic microwave background constraints on dark matter models of the Galactic center 511 keV signal}},
journal = {Physical Review D},
year = {2013},
volume = {87},
number = {1},
pages = {103508},
month = may,
publisher = {American Physical Society},
affiliation = {Department of Physics and Winnipeg Institute for Theoretical Physics, University of Winnipeg, Winnipeg, Manitoba R3B 2E9, Canada},
doi = {10.1103/PhysRevD.87.103508},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:36:34GMT},
date-modified = {2020-07-24T04:58:44GMT},
abstract = {The high positron production rate required to explain the flux of 511 keV gamma rays from the Galactic center has inspired many models in which dark matter creates positrons. These models include the annihilation of light dark matter and the scattering of dark matter with excited states (exciting dark matter). We show that existing cosmic microwave background data robustly constrain such models when the annihilation or scattering cross section is not velocity suppressed depending on the model of the Galactic dark matter halo. Upcoming data from the Planck mission can exclude the fiducial Via Lactea II halo model, which also provides a good fit to the 511 keV morphology. We additionally find combined constraints on exciting dark matter scattering and annihilation, and update constraints on the lifetimes of dark matter excited states. Finally, we apply constraints to models of dark matter decay in which produced positrons fall into the Galactic center and produce the 511 keV signal on their annihilation, demonstrating that most of the parameter space of interest is ruled out.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013PhRvD..87j3508F&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Frey/Physical%20Review%20D%202013%20Frey.pdf},
file = {{Physical Review D 2013 Frey.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Frey/Physical Review D 2013 Frey.pdf:application/pdf;Physical Review D 2013 Frey.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Frey/Physical Review D 2013 Frey.pdf:application/pdf}},
