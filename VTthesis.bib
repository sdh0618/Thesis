@article{Bouchet2015_26Al,
author = {Bouchet, Laurent and Jourdain, Elisabeth and Roques, Jean-Pierre},
title = {{The Galactic 26Al Emission Map as Revealed by INTEGRAL SPI}},
journal = {The Astrophysical Journal},
year = {2015},
volume = {801},
number = {2},
pages = {142},
month = mar,
publisher = {IOP Publishing},
affiliation = {Universit{\'e} de Toulouse, UPS-OMP, IRAP, Toulouse, France; CNRS, IRAP, 9 Av. colonel Roche, BP 44346, F-31028 Toulouse cedex 4, France; lbouchet@irap.omp.eu},
doi = {10.1088/0004-637X/801/2/142},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:19:10GMT},
date-modified = {2020-07-24T03:24:02GMT},
abstract = {Diffuse emission is often challenging since it is undetectable by most instruments, which are generally dedicated to point-source studies. The 26Al emission is a good illustration: the only available 26Al map to date has been released, more than 15 yr ago, thanks to the COMPTEL instrument. However, at the present time, the SPI spectrometer aboard the International Gamma-Ray Astrophysics Laboratory mission offers a unique opportunity to enrich this first result. In this paper, 2 {\texttimes} 108 s of data accumulated between 2003 and 2013 are used to perform a dedicated analysis, aiming to deeply investigate the spatial morphology of the 26Al emission. The data are first compared with several sky maps based on observations at various wavelengths to model the 26Al distribution throughout the Galaxy. For most of the distribution models, the inner Galaxy flux is compatible with a value of 3.3 {\texttimes} 10-4 photons cm-2 s-1, while the preferred template maps correspond to young stellar components such as core-collapse supernovae (SNe), Wolf-Rayet stars, and massive AGB stars. To get more details about this emission, an image reconstruction is performed using an algorithm based on the maximum-entropy method. In addition to the inner Galaxy emission, several excesses suggest that some sites of emission are linked to the spiral arm structure. Lastly, an estimation of the 60Fe line flux, assuming a spatial distribution similar to 26Al line emission, results in a 60Fe-to-26Al ratio around 0.14, which agrees with the most recent studies and with the SN explosion model predictions.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2015ApJ...801..142B&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/801/2/142}}
}

@article{Knoedlseder2004_SPIBG_Cygnus,
author = {Knoedlseder, J and Valsesia, M and Allain, M and Boggs, S and Diehl, R and Jean, P and Kretschmer, K and Roques, J P and Sch{\"o}nfelder, V and Vedrenne, G and von Ballmoos, P and Weidenspointner, G and Winkler, C},
title = {{SPI/INTEGRAL observation of 1809 keV gamma-ray line emission from the Cygnus X region}},
journal = {arXiv.org},
year = {2004},
pages = {arXiv:astro--ph--0405501},
month = may,
annote = {5 pages, 5 figures, accepted for publication in the proceedings of the 5th INTEGRAL workshop (held in Munich, 2004)},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:21:04GMT},
date-modified = {2020-07-24T03:23:49GMT},
abstract = {We present first results on the observation of 1809 keV gamma-ray line emission from the Cygnus X region with the SPI imaging spectrometer. Our analysis is based on data from the performance verification phase of the INTEGRAL instruments and comprises 1.3 Ms of exposure time. We observe a 1809 keV line flux of (7.3 +/- 0.9) x 10-5 ph cm-2 s-1 from a region delimited by galactic longitudes 73-93 deg and |b| < 7 deg at a significance level of 8 sigma. The 1809 keV line appears moderately broadened, with an intrinsic FWHM of 3.3 +/- 1.3 keV. Although this broadening is only marginal (at the 2 sigma level our data are compatible with an unbroadened line), it could reflect the 26Al ejecta kinematics.},
url = {http://adsabs.harvard.edu/abs/2004astro.ph..5501K},
uri = {\url{papers3://publication/uuid/139E8CE5-5F8F-4219-9EDD-D77705E75A15}}
}

