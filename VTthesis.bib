@article{Tuerler2010_CXB_INTEGRAL,
author = {T{\"u}rler, M and Chernyakova, M and Courvoisier, T J L and Lubi{\'{n}}ski, P and Neronov, A and Produit, N and Walter, R},
title = {{INTEGRAL hard X-ray spectra of the cosmic X-ray background and Galactic ridge emission}},
journal = {Astronomy {\&} Astrophysics},
year = {2010},
volume = {512},
pages = {A49},
month = mar,
publisher = {EDP Sciences},
affiliation = {ISDC Data Centre for Astrophysics, ch. d'Ecogia 16, 1290 Versoix, Switzerland ; Geneva Observatory, University of Geneva, ch. des Maillettes 51, 1290 Sauverny, Switzerland marc.turler@unige.ch},
doi = {10.1051/0004-6361/200913072},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:20:17GMT},
date-modified = {2020-07-24T03:23:53GMT},
abstract = {Aims: We derive the spectra of the cosmic X-ray background (CXB) and of the Galactic ridge X-ray emission (GRXE) in the {\textasciitilde}20-200 keV range from the data of the IBIS instrument aboard the INTEGRAL satellite obtained during the four dedicated Earth-occultation observations in early 2006. Methods: We analyze the modulation of the IBIS/ISGRI detector counts induced by the passage of the Earth through the field of view of the instrument. Unlike previous studies, we do not fix the spectral shape of the various contributions, but model instead their spatial distribution and derive for each of them the expected modulation of the detector counts. The spectra of the diffuse emission components are obtained by fitting the normalizations of the model lightcurves to the observed modulation in different energy bins. Because of degeneracy, we guide the fits with a realistic choice of the input parameters and a constraint for spectral smoothness. Results: The obtained CXB spectrum is consistent with the historic HEAO-1 results and falls slightly below the spectrum derived with Swift/BAT. A 10% higher normalization of the CXB cannot be completely excluded, but it would imply an unrealistically high albedo of the Earth. The derived spectrum of the GRXE confirms the presence of a minimum around 80 keV with improved statistics and yields an estimate of {\textasciitilde}0.6 M⊙ for the average mass of white dwarfs in the Galaxy. The analysis also provides updated normalizations for the spectra of the Earth's albedo and the cosmic-ray induced atmospheric emission. Conclusions: This study demonstrates the potential of INTEGRAL Earth-occultation observations to derive the hard X-ray spectra of three fundamental components: the CXB, the GRXE and the Earth emission. Further observations would be extremely valuable to confirm our results with improved statistics.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2010A%26A...512A..49T&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1051/0004-6361/200913072}}
}

