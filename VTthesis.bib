@phdthesis{Kierans2018_PhD,
author = {Kierans, Carolyn},
title = {{Detection of the 511 keV positron annihilation line with the Compton Spectrometer and Imager.}},
school = {UC Berkeley},
year = {2018},
address = {Published online at https://escholarship.org/uc/item/1244t3h7},
publisher = {UC Berkeley},
doi = {https://escholarship.org/uc/item/1244t3h7},
rating = {0},
date-added = {2020-07-24T03:21:10GMT},
date-modified = {2020-07-24T03:23:47GMT},
abstract = {Author(s): Kierans, Carolyn | Advisor(s): Boggs, Steven E | Abstract: The signature of positron annihilation, namely the 511{\textasciitilde}keV $\gamma$-ray line, was first detected coming from the direction of the Galactic center in the 1970's, but the source of Galactic positrons still remains a puzzle. The measured flux of the annihilation corresponds to an intense steady source of positron production, with an annihilation rate of 1043{\textasciitilde}e^+/s. Spatially, the 511{\textasciitilde}keV intensity is strongest in the Galactic center region, with an additional component that is consistent with the Galactic disk; however, the unique morphology is not well constrained.The Compton Spectrometer and Imager (COSI) is a balloon-borne soft $\gamma$-ray (0.2--5{\textasciitilde}MeV) telescope designed to perform wide-field imaging and high-resolution spectroscopy, with a goal of furthering our understanding of Galactic positrons. COSI employs a compact Compton telescope design, using 12 cross-strip germanium detectors to track the trajectory of incident photons, where position and energy deposits from Compton interactions allow for a reconstruction of the source sky position and significant background reduction. COSI had a record-breaking 46-day balloon flight from Wanaka, New Zealand, in May--July 2016, and here we report on the detection and analyses of the 511{\textasciitilde}keV emission from those observations. To isolate the Galactic positron annihilation emission, we have developed a background subtraction technique utilizing the COMPTEL Data Space. With this new method, we find a 7.2{\textasciitilde}$\sigma$ detection of the 511{\textasciitilde} line and a broader spatial distribution of the emission than has been previously reported.},
url = {https://escholarship.org/uc/item/1244t3h7},
uri = {\url{papers3://publication/doi/https://escholarship.org/uc/item/1244t3h7}}
}

