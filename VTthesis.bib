@article{Bouchet2008_imaging,
author = {Bouchet, L and Jourdain, E and Roques, J P and Strong, A and Diehl, R and Lebrun, F and Terrier, R},
title = {{INTEGRAL SPI All-Sky View in Soft Gamma Rays: A Study of Point-Source and Galactic Diffuse Emission}},
journal = {The Astrophysical Journal},
year = {2008},
volume = {679},
number = {2},
pages = {1315--1326},
month = jun,
affiliation = {Centre d'Etude Spatiale des Rayonnements, CNRS, F-31028 Toulouse Cedex 4, France},
doi = {10.1086/529489},
read = {Yes},
rating = {0},
date-added = {2021-06-07T09:11:03GMT},
date-modified = {2021-08-04T08:36:58GMT},
abstract = {We have processed the data accumulated with the INTEGRAL SPI instrument over 4 years ({\textasciitilde}51 Ms) to study the morphology of the Galactic "diffuse" emission in the 20 keV to 8 MeV energy range. To achieve this, we simultaneously derived an all-sky census of emitting sources and images of the Galactic ridge (GR) emission. In the central radian, the resolved point-source emission amounts to 88%, 91%, and 68% of the total in the 25-50, 50-100, and 100-300 keV domains, respectively. We compare the spatial distribution of the GR emission with the distributions obtained from CO and near-IR maps and quantify our results through latitude and longitude profiles. Below 50 keV, the SPI data are better traced by the latter, supporting a stellar origin for this emission. Furthermore, we find that the GR emission spectrum follows a power law with a photon index {\textasciitilde}1.55 above 50 keV, while an additional component is required below that energy. This component shows a cutoff around 30 keV, reinforcing a stellar origin, as proposed by Krivonos et al. The component of the diffuse emission due to e{\textpm} annihilations is extracted simultaneously, leading to the determination of the related parameters (positronium flux and fraction). Specific discussion is devoted to the annihilation-line distribution, since significant emission is detected over a region as large as {\textasciitilde}80{\textdegree} {\texttimes} {\textasciitilde}10{\textdegree}, potentially associated with the disk or halo surrounding the central regions of our Galaxy. Based on observations with INTEGRAL, an ESA project with instruments and science data center funded by ESA member states (especially the PI countries: Denmark, France, Germany, Italy, Spain, and Switzerland), the Czech Republic, and Poland, and with the participation of Russia and the US.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2008ApJ...679.1315B&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2008/Bouchet/The%20Astrophysical%20Journal%202008%20Bouchet.pdf},
file = {{The Astrophysical Journal 2008 Bouchet.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2008/Bouchet/The Astrophysical Journal 2008 Bouchet.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/529489}}
}

