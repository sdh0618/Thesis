@article{Thielemann2018_AwR,
author = {Thielemann, Friedrich-Karl and Diehl, Roland and Heger, Alexander and Hirschi, Raphael and Liebendoerfer, Matthias},
title = {{Massive Stars and Their Supernovae}},
journal = {Astrophysics with Radioactive Isotopes},
year = {2018},
volume = {453},
pages = {173--286},
month = oct,
publisher = {Springer, Cham},
affiliation = {University of Basel f-k.thielemann@unibas.ch},
