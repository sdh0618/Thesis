@article{Juric2008_SDSSstellarnumberdensity_zSun,
author = {Juri{\'c}, Mario and Ivezi{\'c}, {\v Z}eljko and Brooks, Alyson and Lupton, Robert H and Schlegel, David and Finkbeiner, Douglas and Padmanabhan, Nikhil and Bond, Nicholas and Sesar, Branimir and Rockosi, Constance M and Knapp, Gillian R and Gunn, James E and Sumi, Takahiro and Schneider, Donald P and Barentine, J C and Brewington, Howard J and Brinkmann, J and Fukugita, Masataka and Harvanek, Michael and Kleinman, S J and Krzesinski, Jurek and Long, Dan and Neilsen, Eric H Jr and Nitta, Atsuko and Snedden, Stephanie A and York, Donald G},
