@article{Iguaz2021_CGBPBH,
author = {Iguaz, J and Serpico, P D and Siegert, T},
title = {{Isotropic X-ray bound on Primordial Black Hole Dark Matter}},
journal = {arXiv.org},
year = {2021},
eprint = {2104.03145},
eprinttype = {arxiv},
eprintclass = {2104},
pages = {arXiv:2104.03145},
