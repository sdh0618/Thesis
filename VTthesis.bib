@article{Skinner2008_nova511_retrospective,
author = {Skinner, G and Senziani, F and Jean, P and Hernanz, M},
title = {{The Use of the BAT Instrument on Swift for the Detection of Prompt Gamma-Ray Emission from Novae}},
journal = {RS Ophiuchi (2006) and the Recurrent Nova Phenomenon ASP Conference Series},
year = {2008},
