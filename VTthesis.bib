date-modified = {2020-07-24T03:23:54GMT},
abstract = {<i>Aims. <i/>Gamma-ray line emission from the radioactive decay of <sup>26<sup/>Al{\textasciitilde}reflects nucleosynthesis in massive stars and supernovae. We use INTEGRAL <sup>26<sup/>Al{\textasciitilde}measurements to characterize the distribution and characteristics of <sup>26<sup/>Al{\textasciitilde}source regions throughout the Galaxy.<i>Methods. <i/>The spectrometer SPI aboard INTEGRAL has accumulated over five years of data on <sup>26<sup/>Al{\textasciitilde}gamma-ray emission from the Galactic plane. We analyzed these data using suitable instrumental-background models and adopted sky distribution models to produce high-resolution <sup>26<sup/>Al{\textasciitilde}spectra of Galactic emission, spatially resolved along the Galaxy plane.<i>Results. <i/>We detect the <sup>26<sup/>Al{\textasciitilde}line from the inner Galaxy at {\textasciitilde}{\textasciitilde}significance. The line appears narrow, and we constrain broadening in the source regions to <1.3{\textasciitilde}keV (). Different sky distribution models do not significantly affect those large-scale results. The <sup>26<sup/>Al{\textasciitilde}intensity for the inner Galaxy is derived as {\textasciitilde} , consistent with earlier results from COMPTEL and SPI{\textasciitilde}data. This can be translated to an <sup>26<sup/>Al{\textasciitilde}mass of 2.7{\textasciitilde}<i>{\textpm}<i/> 0.7{\textasciitilde} in the Galaxy as a whole. The <sup>26<sup/>Al{\textasciitilde}intensity is also confirmed to be somewhat brighter in the 4th than in the 1st{\textasciitilde}quadrant (ratio {\textasciitilde}1.3{\textasciitilde}<i>{\textpm}<i/> 0.2). <sup>26<sup/>Al{\textasciitilde}spectra separately derived for regions along the Galactic plane show clear line centroid shifts, attributed largely to the Galaxy's large-scale rotation. The <sup>26<sup/>Al{\textasciitilde}line toward the direction of the Aquila region () appears somewhat broadened. Latitudinal variations of <sup>26<sup/>Al{\textasciitilde}emission towards the inner Galaxy are studied, finding a latitudinal scale height of {\textasciitilde}pc for <sup>26<sup/>Al{\textasciitilde}in the inner Galaxy and a hint (3<i>$\sigma$<i/>) of peculiar <sup>26<sup/>Al{\textasciitilde}emission towards the region , .},
url = {https://www.aanda.org/articles/aa/abs/2009/12/aa11175-08/aa11175-08.html},
uri = {\url{papers3://publication/doi/10.1051/0004-6361/200811175}}
}

@article{Martin2012_511,
author = {Martin, P and Strong, A W and Jean, P and Alexis, A and Diehl, R},
title = {{Galactic annihilation emission from nucleosynthesis positrons}},
journal = {Astronomy {\&} Astrophysics},
year = {2012},
volume = {543},
pages = {A3},
month = jul,
publisher = {EDP Sciences},
affiliation = {Max Planck Institut f{\"u}r extraterrestrische Physik (MPE), Postfach 1312, 85741, Garching, Germany; UJF/CNRS, Institut de Plan{\'e}tologie et d'Astrophysique de Grenoble (IPAG), UMR 5274, BP 53, 38041, Grenoble Cedex 9, France pierrick.martin@obs.ujf-grenoble.fr},
doi = {10.1051/0004-6361/201118721},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:55:55GMT},
date-modified = {2020-07-28T04:34:15GMT},
abstract = {Context. The Galaxy hosts a widespread population of low-energy positrons revealed by successive generations of gamma-ray telescopes through a bright annihilation emission from the bulge region, with a fainter contribution from the inner disk. The exact origin of these particles remains currently unknown. Aims: We estimate the contribution to the annihilation signal of positrons generated in the decay of radioactive 26Al, 56Ni and 44Ti. Methods: We adapted the GALPROP propagation code to simulate the transport and annihilation of radioactivity positrons in a model of our Galaxy. Using plausible source spatial distributions, we explored several possible propagation scenarios to account for the large uncertainties on the transport of {\textasciitilde}MeV positrons in the interstellar medium. We then compared the predicted intensity distributions to the INTEGRAL/SPI observations. Results: We obtain similar intensity distributions with small bulge-to-disk ratios, even for extreme large-scale transport prescriptions. At least half of the positrons annihilate close to their sources, even when they are allowed to travel far away. In the high-diffusion, ballistic case, up to 40% of them escape the Galaxy. In proportion, this affects bulge positrons more than disk positrons because they are injected further off the plane in a tenuous medium, while disk positrons are mostly injected in the dense molecular ring. The predicted intensity distributions are fully consistent with the observed longitudinally-extended disk-like emission, but the transport scenario cannot be strongly constrained by the current data. Conclusions: Nucleosynthesis positrons alone cannot account for the observed annihilation emission in the frame of our model. An additional component is needed to explain the strong bulge contribution, and the latter is very likely concentrated in the central regions if positrons have initial energies in the 100 keV - 1 MeV range.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2012A%26A...543A...3M&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2012/Martin/A&A%202012%20Martin.pdf},
file = {{A&A 2012 Martin.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2012/Martin/A&A 2012 Martin.pdf:application/pdf;A&A 2012 Martin.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2012/Martin/A&A 2012 Martin.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361/201118721}}
}

