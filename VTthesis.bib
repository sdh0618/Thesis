title = {{A kilonova as the electromagnetic counterpart to a gravitational-wave source}},
journal = {Nature},
year = {2017},
volume = {551},
number = {7},
pages = {75--79},
month = nov,
publisher = {Nature Publishing Group},
affiliation = {Astrophysics Research Centre, School of Mathematics and Physics, Queens University Belfast, Belfast BT7 1NN, UK; s.smartt@qub.ac.uk},
