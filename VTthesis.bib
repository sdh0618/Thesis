@article{Fuller2018_511NS,
author = {Fuller, George M and Kusenko, Alexander and Radice, David and Takhistov, Volodymyr},
title = {{Positrons and 511 keV radiation as tracers of recent binary neutron star mergers}},
journal = {arXiv.org},
year = {2018},
month = oct,
annote = {5 pages + 1 page supplement, 2 figures; v2: added 511 keV signal observation from dwarf galaxies},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:19:05GMT},
date-modified = {2021-08-04T08:36:49GMT},
abstract = {Neutron-rich material ejected from neutron star-neutron star (NS-NS) and neutron star-black hole (NS-BH) binary mergers is heated by nuclear processes to temperatures of a few hundred keV, resulting in a population of electron-positron pairs. Some of the positrons escape from the outer layers of the ejecta. We show that the population of low-energy positrons produced by NS-NS and NS-BH mergers in the Milky Way can account for the observed 511-keV line from the Galactic center (GC). Moreover, we suggest how positrons and the associated 511-keV emission can be used as tracers of recent mergers. Recent discovery of 511 keV emission from ultra-faint dwarf galaxy Reticulum II, consistent with a rare NS-NS merger event, provides a smoking-gun signature of our proposal.},
url = {http://arxiv.org/abs/1811.00133v2},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2018/Fuller/arXiv%202018%20Fuller.pdf},
