pages = {664--668},
month = aug,
affiliation = {Department of Physics and Astronomy, University of California, Los Angeles, CA 90095, USA},
doi = {10.1126/science.aav8137},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:19:13GMT},
date-modified = {2020-07-24T03:24:02GMT},
abstract = {The general theory of relativity predicts that a star passing close to a supermassive black hole should exhibit a relativistic redshift. In this study, we used observations of the Galactic Center star S0-2 to test this prediction. We combined existing spectroscopic and astrometric measurements from 1995{\textendash}2017, which cover S0-2{\textquoteright}s 16-year orbit, with measurements from March to September 2018, which cover three events during S0-2{\textquoteright}s closest approach to the black hole. We detected a combination of special relativistic and gravitational redshift, quantified using the redshift parameter ϒ. Our result, ϒ = 0.88 {\textpm} 0.17, is consistent with general relativity (ϒ = 1) and excludes a Newtonian model (ϒ = 0) with a statistical significance of 5$\sigma$.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2019Sci...365..664D&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1126/science.aav8137}}
}

