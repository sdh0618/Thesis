@article{Sleator2019_COSI_DEE,
author = {Sleator, Clio C and Zoglauer, Andreas and Lowell, Alexander W and Kierans, Carolyn A and Pellegrini, Nicholas and Beechert, Jacqueline and Boggs, Steven E and Brandt, Terri J and Lazar, Hadar and Roberts, Jarred M and Siegert, Thomas and Tomsick, John A},
title = {{Benchmarking simulations of the Compton Spectrometer and Imager with calibrations}},
journal = {Nuclear Inst. and Methods in Physics Research},
year = {2019},
volume = {946},
pages = {162643},
month = dec,
affiliation = {Space Sciences Laboratory, University of California, Berkeley, CA 94720-7450, USA sleator@berkeley.edu},
doi = {10.1016/j.nima.2019.162643},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:20:15GMT},
date-modified = {2020-07-24T03:23:53GMT},
abstract = {The Compton Spectrometer and Imager (COSI) is a balloon-borne $\gamma$-ray (0.2-5 MeV) telescope designed to study astrophysical sources. COSI employs a compact Compton telescope design utilizing 12 high-purity germanium double-sided strip detectors and is inherently sensitive to polarization. In 2016, COSI was launched from Wanaka, New Zealand and completed a successful 46-day flight on NASA's new Super Pressure Balloon. In order to perform imaging, spectral, and polarization analysis of the sources observed during the 2016 flight, we compute the detector response from well-benchmarked simulations. As required for accurate simulations of the instrument, we have built a comprehensive mass model of the instrument and developed a detailed detector effects engine which applies the intrinsic detector performance to Monte Carlo simulations. The simulated detector effects include energy, position, and timing resolution, thresholds, dead strips, charge sharing, charge loss, crosstalk, dead time, and detector trigger conditions. After including these effects, the simulations closely resemble the measurements, the standard analysis pipeline used for measurements can also be applied to the simulations, and the responses computed from the simulations are accurate. We have computed the systematic error that we must apply to measured fluxes at certain energies, which is 6.3% on average. Here we describe the detector effects engine and the benchmarking tests performed with calibrations.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2019NIMPA.94662643S&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1016/j.nima.2019.162643}}
}

@article{Nishiyama2013_NuclearBulge_PlasmaMagnet,
author = {Nishiyama, Shogo and Yasui, Kazuki and Nagata, Tetsuya and Yoshikawa, Tatsuhito and Uchiyama, Hideki and Sch{\"o}del, Rainer and Hatano, Hirofumi and Sato, Shuji and Sugitani, Koji and Suenaga, Takuya and Kwon, Jungmi and Tamura, Motohide},
title = {{Magnetically Confined Interstellar Hot Plasma in the Nuclear Bulge of Our Galaxy}},
journal = {The Astrophysical Journal Letters},
year = {2013},
volume = {769},
number = {2},
pages = {L28},
month = jun,
publisher = {IOP Publishing},
affiliation = {National Astronomical Observatory of Japan, Mitaka, Tokyo 181-8588, Japan},
doi = {10.1088/2041-8205/769/2/L28},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T04:02:40GMT},
date-modified = {2020-07-28T04:34:14GMT},
abstract = {The origin of the Galactic center diffuse X-ray emission (GCDX) is still under intense investigation. In particular, the interpretation of the hot (kT $\approx$ 7 keV) component of the GCDX, characterized by the strong Fe 6.7 keV line emission, has been contentious. If the hot component originates from a truly diffuse interstellar plasma, not a collection of unresolved point sources, such plasma cannot be gravitationally bound, and its regeneration would require a huge amount of energy. Here, we show that the spatial distribution of the GCDX does not correlate with the number density distribution of an old stellar population traced by near-infrared light, strongly suggesting a significant contribution of the diffuse interstellar plasma. Contributions of the old stellar population to the GCDX are implied to be {\textasciitilde}50% and {\textasciitilde}20% in the nuclear stellar disk (NSD) and nuclear star cluster, respectively. For the NSD, a scale height of 0.{\textdegree}32 {\textpm} 0.{\textdegree}02 is obtained for the first time from the stellar number density profiles. We also show the results of the extended near-infrared polarimetric observations in the central 3{\textdegree} {\texttimes} 2{\textdegree} region of our Galaxy, and confirm that the GCDX region is permeated by a large scale, toroidal magnetic field (MF) as previously claimed. Together with observed MF strengths close to energy equipartition, the hot plasma could be magnetically confined, reducing the amount of energy required to sustain it.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2013ApJ...769L..28N&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Nishiyama/The%20Astrophysical%20Journal%20Letters%202013%20Nishiyama.pdf},
file = {{The Astrophysical Journal Letters 2013 Nishiyama.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Nishiyama/The Astrophysical Journal Letters 2013 Nishiyama.pdf:application/pdf;The Astrophysical Journal Letters 2013 Nishiyama.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2013/Nishiyama/The Astrophysical Journal Letters 2013 Nishiyama.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/2041-8205/769/2/L28}}
}

