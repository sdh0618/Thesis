@article{Laha2019_511PMBHs,
author = {Laha, Ranjan},
title = {{Primordial Black Holes as a Dark Matter Candidate Are Severely Constrained by the Galactic Center 511 keV $\gamma$ -Ray Line}},
journal = {Physical Review Letters},
year = {2019},
volume = {123},
number = {2},
pages = {251101},
month = dec,
publisher = {American Physical Society},
affiliation = {Theoretical Physics Department, CERN, 1211 Geneva 23, Switzerland 0000-0001-7104-5730},
doi = {10.1103/PhysRevLett.123.251101},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:59:00GMT},
date-modified = {2020-07-28T04:34:14GMT},
abstract = {We derive the strongest constraint on the fraction of dark matter that can be composed of low mass primordial black holes by using the observation of the Galactic Center 511 keV $\gamma$ -ray line. Primordial black holes of masses ≲1015 kg will evaporate to produce e{\textpm} pairs. The positrons will lose energy in the Galactic Center, become nonrelativistic, then annihilate with the ambient electrons. We derive robust and conservative bounds by assuming that the rate of positron injection via primordial black hole evaporation is less than what is required to explain the SPI/INTEGRAL observation of the Galactic Center 511 keV $\gamma$ -ray line. Depending on the primordial black hole mass function and other astrophysical uncertainties, these constraints are the most stringent in the literature and show that primordial black holes contribute to less than 1% of the dark matter density. Our technique also probes part of the mass range which was completely unconstrained by previous studies.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2019PhRvL.123y1101L&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2019/Laha/Physical%20Review%20Letters%202019%20Laha.pdf},
file = {{Physical Review Letters 2019 Laha.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2019/Laha/Physical Review Letters 2019 Laha.pdf:application/pdf;Physical Review Letters 2019 Laha.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2019/Laha/Physical Review Letters 2019 Laha.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1103/PhysRevLett.123.251101}}
}

@article{Dobler2012_FermiBubbles_WMAPHaze,
author = {Dobler, Gregory},
title = {{A Last Look at the Microwave Haze/Bubbles with WMAP}},
journal = {The Astrophysical Journal},
year = {2012},
volume = {750},
number = {1},
pages = {17},
month = may,
publisher = {IOP Publishing},
doi = {10.1088/0004-637X/750/1/17},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T04:10:56GMT},
date-modified = {2020-07-28T04:34:10GMT},
abstract = {The microwave "haze" was first discovered with the initial release of the full sky data from the Wilkinson Microwave Anisotropy Probe (WMAP). It is diffuse emission toward the center of our Galaxy with spectral behavior that makes it difficult to categorize as any of the previously known emission mechanisms at those wavelengths. With now seven years of WMAP data publicly available, we have learned much about the nature of the haze, and with the release of data from the Fermi Gamma-Ray Space Telescope and the discovery of the gamma-ray haze/bubbles, we have had a spectacular confirmation of its existence at other wavelengths. As the WMAP mission winds down and the Planck mission prepares to release data, I take a last look at what WMAP has to tell us about the origin of this unique Galactic feature. Much like the gamma rays, the microwave haze/bubbles is/are elongated in latitude with respect to longitude by a factor of roughly two, and at high latitudes, the microwave emission cuts off sharply above {\textasciitilde}35{\textdegree} (compared to {\textasciitilde}50{\textdegree} in the gammas). The hard spectrum of electrons required to generate the microwave synchrotron is consistent with that required to generate gamma-ray emission via inverse Compton scattering, though it is likely that these signals result from distinct regions of the spectrum ({\textasciitilde}10 GeV for the microwaves and {\textasciitilde}1 TeV for the gammas). While there is no evidence for significant haze polarization in the seven-year WMAP data, I demonstrate explicitly that it is unlikely such a signal would be detectable above the noise.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2012ApJ...750...17D&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2012/Dobler/The%20Astrophysical%20Journal%202012%20Dobler.pdf},
file = {{The Astrophysical Journal 2012 Dobler.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2012/Dobler/The Astrophysical Journal 2012 Dobler.pdf:application/pdf;The Astrophysical Journal 2012 Dobler.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2012/Dobler/The Astrophysical Journal 2012 Dobler.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1088/0004-637X/750/1/17}}
}

