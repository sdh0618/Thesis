@article{Pasquali2004_M15,
author = {Pasquali, A and De Marchi, G and Pulone, L and Brigas, M S},
title = {{The global mass function of M 15}},
journal = {Astronomy {\&} Astrophysics},
year = {2004},
volume = {428},
number = {2},
pages = {469--478},
month = dec,
publisher = {EDP Sciences},
affiliation = {ESO/ST-ECF, Karl-Schwarzschild-Strasse 2, 85748 Garching bei M{\"u}nchen, Germany apasqual@eso.org},
doi = {10.1051/0004-6361:20034192},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:19:29GMT},
date-modified = {2020-07-24T03:23:58GMT},
abstract = {Data obtained with the NICMOS instrument on board the Hubble Space Telescope (HST) have been used to determine the H-band luminosity function (LF) and mass function (MF) of three stellar fields in the globular cluster M 15, located {\~{ }}7' from the cluster centre. The data confirm that the cluster MF has a characteristic mass of {\~{ }}0.3 M⊙, as obtained by Paresce {\&} De Marchi (Paresce2000) for a stellar field at 4.6 arcmin from the centre. By combining the present data with those published by other authors for various radial distances (near the centre, at 20'' and at 4.6 arcmin), we have studied the radial variation of the LF due to the effects of mass segregation and derived the global mass function (GMF) using the Michie-King approach. The model that simultaneously best fits the LF at various locations, the surface brightness profile and the velocity dispersion profile suggest that the GMF should resemble a segmented power-law with the following indices: x ≃ 0.8 for stars more massive than 0.8 M⊙, x ≃ 0.9 for 0.3-0.8 M⊙ and x ≃ -2.2 at smaller masses (Salpeter's IMF would have x=1.35). The best fitting model also suggests that the cluster mass is {\~{ }}5.4 {\texttimes} 105 M⊙ and that the mass-to-light ratio is on average M/LV ≃ 2.1, with M/LV ≃ 3.7 in the core. A large amount of mass ({\~{ }}44%) is found in the cluster core in the form of stellar heavy remnants, which may be sufficient to explain the mass segregation in M 15 without invoking the presence of an intermediate-mass black hole.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2004A%26A...428..469P&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1051/0004-6361:20034192}}
}

