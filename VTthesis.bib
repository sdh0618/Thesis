@article{Mori2019_NuSTARJupiterXrays,
author = {Mori, K and Hailey, C J and Branduardi-Raymont, G and Bridges, G L and Clarke, J and Dunn, W and Garvin, A M and Grefenstette, B and Hord, B J and Jackman, C M and Mandel, S and Ray, L C and Wik, D R and Choi, D and Lee, D H and Woo, C},
title = {{NuSTAR hard X-ray observations of the Jovian aurorae during Juno perijove and apojove intervals}},
journal = {American Geophysical Union},
year = {2019},
volume = {43},
month = dec,
affiliation = {Columbia University, New York, NY, United States kaya@astro.columbia.edu},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:19:16GMT},
date-modified = {2020-07-24T03:24:01GMT},
abstract = {We report the detection of hard (> 10 keV) X-ray emission from Jupiter with the Nuclear Spectroscopic Telescope Array (NuSTAR). NuSTAR is the first hard X-ray telescope with sufficient angular resolution (18" FWHM) to remotely resolve the planet. Previous observations with the Ulysses spacecraft had only produced an upper limit on the hard X-ray emission. We carried out a large NuSTAR observation campaign from 2015 to 2018 with 590 ksec total exposure. These NuSTAR observations coincided with one Juno apojove (in June 2017) and three perijoves (in May, July 2017 and April 2018). The observations were coordinated with the Chandra and XMM-Newton X-ray telescopes and HST. NuSTAR detected hard X-ray emission from both the north and south aurorae, which is an extension of the non-thermal electron bremsstrahlung component detected up to 7 keV by XMM-Newton [Branduardi-Raymont et al. 2007]. Broad-band (3-40 keV) X-ray continuum spectra of the Jovian aurorae from NuSTAR, combined with the simultaneous XMM-Newton observations, are well characterized by a power-law model with a very hard photon index ($\Gamma$ {\textasciitilde} 1) and a spectral cutoff above {\textasciitilde}20 keV, required to be consistent with the Ulysses upper limits. Most intriguingly, NuSTAR observed that hard X-ray emission from the south aurora was consistently brighter than its counterpart in the north, whereas soft X-ray emission below 2 keV (where the emission lines from precipitating ions are predominant) is brighter in the north aurora [Gladstone et al. 2002]. We probe the anisotropic hard X-ray emission between the two aurorae using a 3D particle injection simulation tool which incorporates the magnetic field data obtained by Juno's MAG instrument [Moore et al. 2018]. We also present our investigation of whether the electron energy spectra measured in-situ by Juno's JADE/JEDI instruments can account for the X-ray spectra observed by NuSTAR and XMM-Newton.},
url = {http://adsabs.harvard.edu/abs/2019AGUFMSM43B..05M},
uri = {\url{papers3://publication/uuid/A9052D60-6603-4613-8202-11E0B537796C}}
}

