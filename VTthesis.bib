@article{2018MNRAS.480.5657B,
author = {Bray, J C and Eldridge, J J},
title = {{Neutron star kicks - II. Revision and further testing of the conservation of momentum `kick' model}},
journal = {Monthly Notices of the Royal Astronomical Society},
year = {2018},
volume = {480},
number = {4},
pages = {5657--5672},
month = nov,
affiliation = {Department of Physics, University of Auckland, Private Bag 92019, Auckland 1010, New Zealand},
doi = {10.1093/mnras/sty2230},
language = {English},
rating = {0},
date-added = {2020-07-24T03:20:49GMT},
date-modified = {2020-07-24T03:23:50GMT},
abstract = {In Bray {\&} Eldridge, we proposed a simple neutron star (NS) `kick' formula, v_kick=$\alpha$ (M_ejecta / M_remnant) + $\beta$ to explain the observed 2D velocities of young single NSs. Using this kick we found that there is no statistically significant preference for a kick orientation nor for any of the three initial mass function (IMF) slopes tested, and that populations including binary stars reproduced the kick distribution better than single-star only populations. However, recent analysis by Janka prompted us to revisit our basic assumptions and our new analysis has led to revised `best-fitting' kick values of $\alpha$ =100 km s^-1 and $\beta$ =-170 km s^-1. The reduction of $\beta$ to a negative value is due to using the 2D observed kick velocity distribution rather than the modelled 3D velocity distribution for NSs. To further test the validity of the new kick, we have created synthetic populations of runaway star and double neutron star (DNS) binaries at solar metallicity (Z = 0.020) using our best-fitting kick. We find our new kick values create runaway star velocities and DNS period distributions in agreement with the comparable observational distributions with only the DNS eccentricities in tension with the observations. From our DNS and black hole-black hole (BH-BH) data sets we estimate a predicted DNS merger rate at solar metallicity of 3864^+1,570_-2,371 Gpc-3 yr-1 and a BH-BH merger rate of 5^+40_-1 Gpc-3 yr-1.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2018MNRAS.480.5657B&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1093/mnras/sty2230}}
}

@article{Boggs2015_SN1987A,
author = {Boggs, S E and Harrison, F A and Miyasaka, H and Grefenstette, B W and Zoglauer, A and Fryer, C L and Reynolds, S P and Alexander, D M and An, H and Barret, D and Christensen, F E and Craig, W W and Forster, K and Giommi, P and Hailey, C J and Hornstrup, A and Kitaguchi, T and Koglin, J E and Madsen, K K and Mao, P H and Mori, K and Perri, M and Pivovaroff, M J and Puccetti, S and Rana, V and Stern, D and Westergaard, N J and Zhang, W W},
