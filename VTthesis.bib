@article{Kinzer2001_511,
author = {Kinzer, R L and Milne, P A and Kurfess, J D and Strickman, M S and Johnson, W N and Purcell, W R},
title = {{Positron Annihilation Radiation from the Inner Galaxy}},
journal = {The Astrophysical Journal},
year = {2001},
volume = {559},
number = {1},
pages = {282--295},
month = sep,
publisher = {IOP Publishing},
affiliation = {Naval Research Laboratory, Washington, DC 20375-5352.},
doi = {10.1086/322371},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T04:12:39GMT},
date-modified = {2020-07-28T04:34:09GMT},
abstract = {The OSSE instrument on the Compton Gamma Ray Observatory (CGRO) was used to measure the one-dimensional latitudinal and longitudinal distributions of positron annihilation radiation (annihilation line and positronium continuum components) along the inner Galactic ridge. Intensity measurements near the Galactic center (differential relative to background fields offset by 9{\textdegree}-12{\textdegree}) show a symmetrical and spatially narrow bulge around the center (effective FWHMs of 4.9d+/-0.7d in Galactic latitude and 6.3d+/-1.5d in longitude) with significant disk contributions to at least 35{\textdegree} in longitude. The latitude width of the Galactic plane emission away from the Galactic center is only weakly constrained in the current analysis. The longitudinal distribution is well fitted by either a model comprising a narrow ({\textasciitilde}6.3d FWHM ) Gaussian bulge plus {\textasciitilde}35{\textdegree} FWHM Gaussian and CO-like disk components, or by a center-truncated R1/4 spheroid plus exponential disk model. For the latter model, the observed narrow latitudinal distribution near the Galactic center suggests that the bulge is ellipsoidal, although the fits are consistent with a spherical bulge as well. The positronium fraction estimate from the Galactic center direction is 0.93+/-0.04. This, combined with the existing narrow line width measurements for the 511 keV annihilation line, suggests that the bulk of the positron annihilation from the Galactic center direction occurs after positronium formation in a warm environment. The inner Galaxy total annihilation radiation flux intensities of {\textasciitilde}10.6{\texttimes}10-3 to {\textasciitilde}13.7{\texttimes}10-3 cm-2 s-1 implied by the fitted R1/4 ellipsoid plus exponential disk model (narrow and broad disks, respectively) are in agreement with intensities implied by the 511 keV line flux observed by the broad-field SMM instrument. The double-Gaussian plus CO disk model gives similar agreement if a broad ({\textasciitilde}12{\textdegree} FWHM) disk component is assumed.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2001ApJ...559..282K&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2001/Kinzer/The%20Astrophysical%20Journal%202001%20Kinzer.pdf},
file = {{The Astrophysical Journal 2001 Kinzer.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2001/Kinzer/The Astrophysical Journal 2001 Kinzer.pdf:application/pdf;The Astrophysical Journal 2001 Kinzer.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2001/Kinzer/The Astrophysical Journal 2001 Kinzer.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1086/322371}}
}

@article{Loredo2019_popsyn,
author = {Loredo, Thomas J and Hendry, Martin A},
title = {{Multilevel and hierarchical Bayesian modeling of cosmic populations}},
journal = {arXiv.org},
year = {2019},
eprint = {1911.12337},
eprinttype = {arxiv},
eprintclass = {1911},
pages = {arXiv:1911.12337},
month = nov,
annote = {33 pages, 5 figures},
read = {Yes},
rating = {0},
date-added = {2021-04-07T12:39:42GMT},
date-modified = {2021-09-08T12:48:38GMT},
abstract = {Demographic studies of cosmic populations must contend with measurement errors and selection effects. We survey some of the key ideas astronomers have developed to deal with these complications, in the context of galaxy surveys and the literature on corrections for Malmquist and Eddington bias. From the perspective of modern statistics, such corrections arise naturally in the context of multilevel models, particularly in Bayesian treatments of such models: hierarchical Bayesian models. We survey some key lessons from hierarchical Bayesian modeling, including shrinkage estimation, which is closely related to traditional corrections devised by astronomers. We describe a framework for hierarchical Bayesian modeling of cosmic populations, tailored to features of astronomical surveys that are not typical of surveys in other disciplines. This thinned latent marked point process framework accounts for the tie between selection (detection) and measurement in astronomical surveys, treating selection and measurement error effects in a self-consistent manner.},
url = {http://adsabs.harvard.edu/abs/2019arXiv191112337L},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2019/Loredo/arXiv%202019%20Loredo.pdf},
