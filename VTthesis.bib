@article{Jean2003_SPIBG,
author = {Jean, P and Vedrenne, G and Roques, J P and Sch{\"o}nfelder, V and Teegarden, B J and von Kienlin, A and Knoedlseder, J and Wunderer, C and Skinner, G K and Weidenspointner, G and Atti{\'e}, D and Boggs, S and Caraveo, P and Cordier, B and Diehl, R and Gros, M and Leleux, P and Lichti, G G and Kalemci, E and Kiener, J and Lonjou, V and Mandrou, P and Paul, Ph and Schanne, S and von Ballmoos, P},
title = {{SPI instrumental background characteristics}},
journal = {Astronomy {\&} Astrophysics},
year = {2003},
