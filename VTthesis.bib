@article{Fujii2005_CRheliosphere,
author = {Fujii, Z and McDonald, F B},
title = {{The spatial distribution of galactic and anomalous cosmic rays in the heliosphere at solar minimum}},
journal = {Advances in Space Research},
year = {2005},
volume = {35},
number = {4},
pages = {611--616},
affiliation = {Solar Terrestrial Environment Laboratory, Nagoya University, Chikusa-ku, Nagoya 464-8601, Japan; Institute for Physical Science and Technology, University of Maryland, College Park, MD 20742, USA},
doi = {10.1016/j.asr.2005.01.023},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:19:08GMT},
date-modified = {2020-07-24T03:24:03GMT},
abstract = {The spatial distributions of galactic and anomalous cosmic rays in the heliosphere at the solar minima of Cycles 20/22 (qA > 0) and of Cycle 21 (qA < 0) are studied, using data from IMP 8, Voyagers 1/2 and Pioneer 10. It is found that the radial dependences of intensities J can be approximated by a power of radial distance r as J $\propto$ r$\alpha$ with a different value of a constant in the inner and outer heliosphere with a transition at a radial distance of 10-15 AU. To study the physical meaning of these radial intensity profiles we examined the rigidity dependences of the intensity gradients by determining the particle mean free paths, using a simple one-dimensional modulation model. The particle mean free path $\lambda$ was assumed to be a separable function of distance of the form r$\gamma$ and rigidity R of R$\delta$ over the range of 0.5-3.0 GV in the inner and outer heliosphere. It was shown that $\lambda$ of rigidity dependence of R1.6 determined for Cycle 20/22 (qA > 0) with anomalous He is about 4-5 times larger than that of Cycle 21 (qA < 0) with R0.9 at around 1 GV in the outer heliosphere, and that the radial dependences are r1.4 and r1.1, respectively, for Cycles 20/22 and for Cycle 21.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2005AdSpR..35..611F&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1016/j.asr.2005.01.023}}
}

