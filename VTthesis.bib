@article{Siegert2016_dsph,
author = {Siegert, Thomas and Diehl, Roland and Vincent, Aaron C and Guglielmetti, Fabrizia and Krause, Martin G H and Boehm, Celine},
title = {{Search for 511 keV emission in satellite galaxies of the Milky Way with INTEGRAL/SPI}},
journal = {Astronomy {\&} Astrophysics},
year = {2016},
volume = {595},
pages = {A25},
month = oct,
publisher = {EDP Sciences},
affiliation = {Max-Planck-Institut f{\"u}r extraterrestrische Physik, Gie{\ss}enbachstra{\ss}e, 85741, Garching, Germany tsiegert@mpe.mpg.de},
doi = {10.1051/0004-6361/201629136},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:20:28GMT},
date-modified = {2021-08-04T08:36:57GMT},
abstract = {Context. The positron (e+) annihilation $\gamma$-ray signal in the Milky Way (MW) shows a puzzling morphology: a very bright bulge and a very low surface-brightness disk. A coherent explanation of the e+ origin, propagation through the Galaxy and subsequent annihilation in the interstellar medium has not yet been found. Tentative explanations involve e+s from radioactivity, X-ray binaries, and dark matter (DM). Aims: Dwarf satellite galaxies (DSGs) are believed to be dominated by DM and hence are promising candidates in the search for 511 keV emission as a result of DM annihilation into e+e--pairs. The goal of this study is to constrain possible 511 keV $\gamma$-ray signals from 39 DSGs of the MW and to test the annihilating DM scenario. Methods: We used the spectrometer SPI on INTEGRAL to extract individual spectra for the studied objects in the range 490-530 keV. As the diffuse galactic 511 keV emission dominates the overall signal, we modelled the large-scale morphology of the MW accordingly and included this in a maximum likelihood analysis. Alternatively, a distance-weighted stacked spectrum was determined, representing an average DSG seen in 511 keV. Results: Only Reticulum II (Ret II) shows a 3.1$\sigma$ signal. Five other sources show tentative 2$\sigma$ signals. The ratio of mass to 511 keV luminosity, $\Upsilon$511, shows a marginal trend towards higher values for intrinsically brighter objects in contrast to the mass-to-light ratio, $\Upsilon$V in the V band, which is generally used to uncover DM in DSGs. Conclusions: All derived 511 keV flux values or upper limits are above the flux level implied by a DM interpretation of the MW bulge signal. The signal detected from Ret II is unlikely to be related to a DM origin alone, otherwise, the MW bulge would be {\textasciitilde}100 times brighter in 511 keV than what is seen with SPI. Ret II is exceptional considering the DSG sample and rather points to enhanced recent star formation activity if its origins are similar to processes in the MW. Understanding this emission may provide further clues regarding the origin of the annihilation emission in the MW bulge.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2016A%26A...595A..25S&link_type=EJOURNAL},
local-url = {file://localhost/Users/thomassiegert/Papers/Library.papers3/Articles/2016/Siegert/A&A%202016%20Siegert.pdf},
file = {{A&A 2016 Siegert.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2016/Siegert/A&A 2016 Siegert.pdf:application/pdf;A&A 2016 Siegert.pdf:/Users/thomassiegert/Papers/Library.papers3/Articles/2016/Siegert/A&A 2016 Siegert.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1051/0004-6361/201629136}}
