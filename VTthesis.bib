@article{2004A&A...422L..47S,
author = {Strong, A W and Moskalenko, I V and Reimer, O and Digel, S and Diehl, R},
title = {{The distribution of cosmic-ray sources in the Galaxy, $\gamma$-rays and the gradient in the CO-to-H2 relation}},
