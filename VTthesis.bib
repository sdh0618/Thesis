@article{Su2010_fermibubbles,
author = {Su, Meng and Slatyer, Tracy R and Finkbeiner, Douglas P},
title = {{Giant Gamma-ray Bubbles from Fermi-LAT: Active Galactic Nucleus Activity or Bipolar Galactic Wind?}},
journal = {The Astrophysical Journal},
year = {2010},
volume = {724},
number = {2},
pages = {1044--1082},
month = dec,
publisher = {IOP Publishing},
affiliation = {Institute for Theory and Computation, Harvard-Smithsonian Center for Astrophysics, 60 Garden Street, MS-51, Cambridge, MA 02138, USA mengsu@cfa.harvard.edu},
