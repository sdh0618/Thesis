@article{Tsygankov2016_44Ti,
author = {Tsygankov, Sergey S and Krivonos, Roman A and Lutovinov, Alexander A and Revnivtsev, Mikhail G and Churazov, Eugene M and Sunyaev, Rashid A and Grebenev, Sergey A},
title = {{Galactic survey of 44Ti sources with the IBIS telescope onboard INTEGRAL}},
journal = {Monthly Notices of the Royal Astronomical Society},
year = {2016},
volume = {458},
number = {4},
pages = {3411--3419},
month = jun,
affiliation = {Tuorla Observatory, Department of Physics and Astronomy, University of Turku, V{\"a}is{\"a}l{\"a}ntie 20, FI-21500 Piikki{\"o}, Finland; Space Research Institute of the Russian Academy of Sciences, Profsoyuznaya Str. 84/32, Moscow 117997, Russia; stsygankov@gmail.com},
doi = {10.1093/mnras/stw549},
language = {English},
read = {Yes},
rating = {0},
date-added = {2020-07-24T03:20:21GMT},
date-modified = {2020-07-24T03:23:52GMT},
abstract = {We report the results of the deepest Galactic plane (|b| < 17.5{\textdegree}) survey in the 67.9 and 78.4 keV nuclear de-excitation lines of titanium-44 (44Ti) performed using the data acquired with the IBIS/ISGRI instrument onboard the INTEGRAL satellite during 12 yr of operation. The peak sensitivity of our survey reached an unprecedented level of 4.8 {\texttimes} 10-6 ph cm-2 s-1 (3$\sigma$) that improves the sensitivity of the survey done by Compton Gamma-Ray Observatory/COMPTEL by a factor of {\~{ }}5. As a result, constraining upper limits for all sources from the catalogue of Galactic supernova remnants (SNRs; Green 2014) are derived. These upper limits can be used to estimate the exposure needed to detect 44Ti emission from any known SNR using existing and prospective X- and gamma-ray telescopes. Among the youngest Galactic SNRs, only Cas A shows significant 44Ti emission flux in good agreement with the NuSTAR measurements. We did not detect any other sources of titanium emission in the Galactic plane at significance level higher than 5$\sigma$ confirming previous claims of the rarity of such 44Ti-producing SNRs.},
url = {http://adsabs.harvard.edu/cgi-bin/nph-data_query?bibcode=2016MNRAS.458.3411T&link_type=EJOURNAL},
uri = {\url{papers3://publication/doi/10.1093/mnras/stw549}}
}

